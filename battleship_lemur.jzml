<SCRIPT name="on(MIDI_ARGS)" script="// constants
decl COL_PLACED = 1596013;
decl LBL_PLACED = '';

// set ON color
decl color = getattribute(MyBoard, 'color');
color[1] = COL_PLACED;
setattribute(MyBoard, 'color', color);

// set LABEL
decl i = MIDI_ARGS[0];
decl labels = getattribute(MyBoard, 'labels');
labels[i] = LBL_PLACED;
setattribute(MyBoard, 'labels', labels);

// set ON state
MyBoard.x[i] = 1;" trigger_script="" trigger_type="3" trigger="1" osc_message="/MyBoard/on" midi_message="0x90,0x93,0,24" midi_target="0" flag="1"/>
<SCRIPT name="off(MIDI_ARGS)" script="decl CH_EMPTY = 1, CH_MISS = 2, CH_HIT = 3;
decl COL_EMPTY = 865343, COL_MISS = COL_EMPTY, COL_HIT = 4130354;
decl LBL_EMPTY = '', LBL_MISS = 'o', LBL_HIT = LBL_EMPTY;

decl i = MIDI_ARGS[0], ch = MIDI_ARGS[2];
decl colors = getattribute(MyBoard, 'colors'), labels = getattribute(MyBoard, 'labels');
decl color, label;
if (ch == CH_EMPTY) {
  color = COL_EMPTY;
  label = LBL_EMPTY;
} else if (ch == CH_MISS) {
  color = COL_MISS;
  label = LBL_MISS;
} else if (ch == CH_HIT) {
  color = COL_HIT;
  label = LBL_HIT;
}
colors[i] = color;
labels[i] = label;
setattribute(MyBoard, 'colors', colors);
setattribute(MyBoard, 'labels', labels);

MyBoard.x[i] = 0;" trigger_script="" trigger_type="3" trigger="1" osc_message="/MyBoard/off" midi_message="0x80,0x83,0,24" midi_target="0" flag="1"/>
